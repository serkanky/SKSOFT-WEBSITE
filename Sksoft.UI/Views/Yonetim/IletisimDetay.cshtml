@model Sksoft.Entity.Iletisim

@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<td>
    <a href="@Url.Action("Iletisim", "Yonetim")" class="btn btn-default">İletişim Talepleri</a>
</td>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Adı Soyadı</th>
            <th scope="col">Mesaj</th>
            <th scope="col">Telefon</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>
                <div class="col-4">
                    <textarea class="form-control mb-10" rows="1" disabled cols="5">@Model.AdiSoyadi</textarea>
                </div>
            </td>
        </tr>
        <tr>
            <td>
                <div class="col-4">
                    <textarea class="form-control mb-10" rows="1" disabled cols="5">@Model.Telefon</textarea>
                </div>
            </td>
        </tr>
        <tr>
            <td>
                <div class="col-4">
                    <textarea class="form-control mb-10" rows="1" disabled cols="5">@Model.Mesaj</textarea>
                </div>
            </td>
        </tr>
    </tbody>
</table>